@model Hitchhikers.Models.Picture

 <div class="container">
    <div class="col">
      <h1>Welcome to @ViewBag.state</h1>
    </div>
    <div class="col"></div>
    <div class="col"></div>
    <div class="row mt-5">
      @{ 
          
        foreach (var pictures in @ViewBag.DisplayPhoto)
        {
          <div class="col-sm-12 col-md-3">
            <div class="thumbnail">
              <img src="~/images/@pictures.PictName" alt="image_pic">
              <div class="caption">
                <h3>@pictures.Description</h3>
                <p>City: @pictures.City</p>
                <p>State: @pictures.States</p>
              </div>
            </div>
          </div>
        }
      }
    </div>
    <div class="row">
      <div class="col-md-12 text-right">
          <form asp-controller="Travel" asp-action="Create" method="get">
              <button type="submit" class="btn btn-primary">Add new picture</button>
              <input type="hidden" value="CollectivePhotos" name="Page">
          </form>
      </div>
    </div>
</div>


